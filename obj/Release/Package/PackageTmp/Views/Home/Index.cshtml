@using WebApplication1.Models
@model IEnumerable<SanPham>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Layout/HomeLayout.cshtml";
}

@{
    IEnumerable<SanPham> lstDTM = (IEnumerable<SanPham>)ViewBag.ListDTM;
    IEnumerable<SanPham> lstLTM = (IEnumerable<SanPham>)ViewBag.ListLTM;
    IEnumerable<SanPham> lstMTBM = (IEnumerable<SanPham>)ViewBag.ListMTBM;


}


<div class="content-top">
    <h1>Điện thoại mới nhất</h1>
    <div class="grid-in">
        <div class="container">
            @Html.Partial("~/Views/SanPham/SanPhamStylePartial.cshtml", lstDTM);
        </div>
    </div>

    <h1>Máy tính xách tay mới nhất </h1>
    <div class="grid-in">
        <div class="container">
            @Html.Partial("~/Views/SanPham/SanPhamStyle2Partial.cshtml", lstLTM);
        </div>
    </div>

    <h1>Máy tính bảng mới nhất</h1>
    <div class="grid-in">
        <div class="container">
            @Html.Partial("~/Views/SanPham/SanPhamStylePartial.cshtml", lstMTBM);
        </div>
    </div>
</div>
